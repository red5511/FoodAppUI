          "name": { "type": "string" },
          "address": { "type": "string" },
          "openHours": { "$ref": "#/components/schemas/OpenHours" },
          "companyType": { "type": "string", "enum": ["FOOD"] },
          "webSocketTopicName": { "type": "string" },
          "receivingOrdersActive": { "type": "boolean" }
        }
      },
      "LocalTime": {
        "type": "object",
        "properties": {
          "hour": { "type": "integer", "format": "int32" },
          "minute": { "type": "integer", "format": "int32" },
          "second": { "type": "integer", "format": "int32" },
          "nano": { "type": "integer", "format": "int32" }
        }
      },
      "ModifyCompanyRequest": {
        "type": "object",
        "properties": {
          "companyDto": { "$ref": "#/components/schemas/CompanyDto" }
        }
      },
      "OpenHours": {
        "type": "object",
        "properties": {
          "mondayStart": { "$ref": "#/components/schemas/LocalTime" },
          "mondayEnd": { "$ref": "#/components/schemas/LocalTime" },
          "tuesdayStart": { "$ref": "#/components/schemas/LocalTime" },
          "tuesdayEnd": { "$ref": "#/components/schemas/LocalTime" },
          "wednesdayStart": { "$ref": "#/components/schemas/LocalTime" },
          "wednesdayEnd": { "$ref": "#/components/schemas/LocalTime" },
          "thursdayStart": { "$ref": "#/components/schemas/LocalTime" },
          "thursdayEnd": { "$ref": "#/components/schemas/LocalTime" },
          "fridayStart": { "$ref": "#/components/schemas/LocalTime" },
          "fridayEnd": { "$ref": "#/components/schemas/LocalTime" },
          "saturdayStart": { "$ref": "#/components/schemas/LocalTime" },
          "saturdayEnd": { "$ref": "#/components/schemas/LocalTime" },
          "sundayStart": { "$ref": "#/components/schemas/LocalTime" },
          "sundayEnd": { "$ref": "#/components/schemas/LocalTime" }
        }
      },
      "InitOrderWebSocketTopicRequest": {
        "required": ["companyId", "webSocketTopicName"],
        "type": "object",
        "properties": {
          "companyId": {
            "type": "integer",
            "description": "company id",
            "format": "int64"
          },
          "webSocketTopicName": {
            "type": "string",
            "description": "websocket topic name"
          }
        }
      },
      "GetStatisticsConfigRequest": {
        "type": "object",
        "properties": {
          "companyIds": {
            "type": "array",
            "items": { "type": "integer", "format": "int64" }
          }
        }
      },
      "DatePeriodModel": {
